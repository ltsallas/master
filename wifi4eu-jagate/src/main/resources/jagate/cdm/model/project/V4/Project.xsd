				<xs:annotation>
					<xs:documentation>forms of costs actually used in the grant - for H2020
						values: {actual costs, unit costs, flat-rate costs, lump sum costs}
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="YearsToEvaluateImpact" type="xs:string" minOccurs="0">
