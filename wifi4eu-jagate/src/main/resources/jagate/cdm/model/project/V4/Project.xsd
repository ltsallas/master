s:ERC="http://ec.europa.eu/research/fp/model/project-erc/V4" xmlns:MarieCurie="http://ec.europa.eu/research/fp/model/project-mariecurie/V5" xmlns:RFCS="http://ec.europa.eu/research/fp/model/project-rfcs/V1" xmlns:AGRIP="http://ec.europa.eu/research/fp/model/project-agrip/V1" xmlns:IMI2="http://ec.europa.eu/research/fp/model/project-imi2/V1" xmlns:delSubmission="http://ec.europa.eu/research/fp/model/project-deliverable-submission/V1" xmlns:project-ethics="http://ec.europa.eu/research/fp/model/project-ethics/V1" xmlns:invoice="http://ec.europa.eu/research/fp/model/invoice/V1" xmlns:reportassessment="http://ec.europa.eu/research/fp/model/report-assessment/V1" xmlns:projecttermination="http://ec.europa.eu/research/fp/model/project-termination/V1" targetNamespace="http://ec.europa.eu/research/fp/model/project/V4" elementFormDefault="qualified" version="4.9.20">
