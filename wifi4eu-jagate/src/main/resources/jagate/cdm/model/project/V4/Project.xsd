	<xs:element name="Project" type="ProjectType"/>
	<xs:complexType name="ProjectType">
		<xs:sequence>
			<xs:group ref="projectref:ProjectIdGroup"/>
			<xs:group ref="GeneralGroup" minOccurs="0"/>
			<xs:group ref="LifeCycleGroup" minOccurs="0"/>
			<xs:group ref="GrantingResponsibilityGroup" minOccurs="0"/>
			<xs:group ref="ConsortiumGroup" minOccurs="0"/>
			<xs:group ref="SpecialClausesGroup" minOccurs="0"/>
			<xs:group ref="FinancialGroup" minOccurs="0"/>
			<xs:group ref="FundingSchemeGroup" minOccurs="0"/>
			<xs:group ref="WorkPlanningGroup" minOccurs="0"/>
			<xs:group ref="ReportingGroup" minOccurs="0"/>
			<xs:group ref="proposal:ProposalGroup" minOccurs="0"/>
			<xs:element name="Ethics" type="project-ethics:EthicsRequirementType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="InvitationLetter" type="invitationletter:InvitationLetterType" minOccurs="0"/>
			<xs:element name="GrantAgreement" type="GrantAgreementType" minOccurs="0"/>
			<xs:element name="AdministrativeArrangement" type="AdministrativeArrangementType" minOccurs="0"/>
			<xs:element name="IndividualCommitments" type="IndividualCommitmentListType" minOccurs="0"/>
			<xs:element name="NegotiationReport" type="negoreport:NegotiationReportType" minOccurs="0" maxOccurs="2"/>
			<xs:element name="RankingInformation" type="RankingInformationType" minOccurs="0"/>
			<xs:element name="CommissionDecision" type="CommissionDecisionType" minOccurs="0"/>
			<xs:element name="Amendment" type="projectamendment:AmendmentType" minOccurs="0"/>
			<xs:element name="GrantTermination" type="projecttermination:GrantTerminationType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:group name="GeneralGroup">
		<xs:sequence>
			<xs:group ref="core:GeneralCoreGroup"/>
			<xs:element name="SpecificProgramme" type="coderef:CodeRefType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specific programme is the FPDivision code of level 3</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Theme" type="coderef:CodeRefType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Theme is the FPDivision code of level 4</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EthicsClearance" type="EthicsClearanceType" minOccurs="0"/>
			<xs:element name="LinkedProject" type="LinkedProjectType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="TreatJrcAsNormalPartner" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>this flas is set to true for JU where JRC should be treated as a normal partner. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OpenAccessData" type="OpenAccessDataType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>open access data pilot information. 
					STAY_IN means that the call/topic is IN the pilot and the project stays in.
					OPT_OUT means that the call/topic is IN the pilot but the project opts out.
					STAY_OUT means that the call/topic is OUT of the pilot and the project stays out.
					OPT_IN means that the call/topic is OUT of the pilot but the project opts in.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CallKind" type="coderef:CodeRefType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>codes of CCM2 classId CallKindValues</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SubProgramme" type="coderef:CodeRefType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Sub programme is the FPDivision code of level 2</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:group name="LifeCycleGroup">
		<xs:sequence>
			<xs:group ref="core:LifeCycleCoreGroup"/>
			<xs:element name="SubmissionDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date when the project proposal has been submitted.
The individual last submission date of a given proposal. This shows when the proposal was actually submitted by the applicant. The fact that the proposal was submitted at that time, does not necessarily mean that it would legally be available to the EC at that time;
NOTE THAT a SubmissionDateTime element is also present for systems needing a precise timestamp.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StartDateChoice" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>FP7/H2020 Choices for the start date of the project:	FIXED_DATE, FIRST_DAY_OF_MONTH_AFTER_SIGN_DATE, EFFECTIVE_START_TO_BE_NOTIFIED_BY_COORD_DATE, ENTRY_INTO_FORCE_DATE
(FIXED_DATE is the project start on a specific date which is fixed from the beginning (before the signature), 		FIRST_DAY_OF_MONTH_AFTER_SIGN_DATE is the first day of the month after the entry into force of the grant agreement,
EFFECTIVE_START_TO_BE_NOTIFIED_BY_COORD_DATE is the starting date will be notified by the coordinator/beneficiary within a specific number of months after the date the grant agreement enters into
force  ENTRY_INTO_FORCE_DATE is the date of the latest signature between coordinator and service. This is for FPA type of actions</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StartDateJustification" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Justification for the fixed start date.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NotificationForStartDateInMonths" type="xs:int" minOccurs="0"/>
			<xs:element name="ProjectRejectionReason" type="coderef:CodeRefType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>use this instead of RejectionReason element</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RejectionReason" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>deprecated:The reason that the project is rejected</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DecisionDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>TODO: Date when EC decision is taken to fund the
						project</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TerminationDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date when project is terminated
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TerminationJustification" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Justification of the termination
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NegotiationStartDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date when negotiation of the project is started
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NegotiationCompletionDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date when negotiation of the project is completed
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CancellationDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date when project is cancelled</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ClosureDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date when project is closed TODO: is it
						considered closed by all RDG business when the final payment has
						been done?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EndDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date when project ends: derived from Start
						Date+duration</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProjectSuspensions" type="ProjectSuspensionList" minOccurs="0">
				<xs:annotation>
					<xs:documentation/>
				</xs:annotation>
			</xs:element>
			<xs:element name="SubmissionDateTime" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The individual last submission timestamp of a given proposal. This shows when the proposal was actually submitted by the applicant. The fact that the proposal was submitted at that time, does not necessarily mean that it would legally be available to the EC at that time.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EffectiveClosureDateTime" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>the individual point in time after which a given proposal can be legally made available to the EC, i.e. after which the proposal is regarded as being "finally" submitted, the CallPhaseEvent (phase Submission) is emitted and the proposal can be harvested for evaluation.	 	
It is defined as follows:
o	Standard calls or 2 stages calls: call deadline;
o	Calls with multiple cut-offs: cut-off date at which one the submitted proposal was harvested. For proposals still in draft at cut-off time, the closure date moves to the next cut-off;
o	Continuous calls: submission date.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DeadlineDateTime" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>the common date for all proposals that belong in the same deadline "container" – usually to be ranked together after evaluation. This is the date typically used to identify this proposal "container" in reporting (CORDA) etc</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:group name="GrantingResponsibilityGroup">
		<xs:sequence>
			<xs:element name="ResponsibleDGCode" type="coderef:CodeRefType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>code of type ECHierarchy (e.g. RTD, CNECT)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ResponsibleDirectorateCode" type="coderef:CodeRefType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>code of type ECHierarchy (e.g. TREN/C)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ResponsibleDepartmentCode" type="coderef:CodeRefType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>code of type ECHierarchy (e.g. TREN/C/02)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Officer" type="ProjectOfficerType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>List of officers associated to the project, eg.
						Project Officer, Financial Officer, etc</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GrantingAuthority" type="coderef:CodeRefType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>code of type GrantingAuthority</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:group name="ConsortiumGroup">
		<xs:sequence>
			<xs:element name="Partner" type="partner:LegalPartnerType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The Partner data that is expected in the context
						of a project related service is the one that is stored inside the
						project backend, not the one that is stored in the PDM system.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ClusterPresent" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>For H2020 CS2 JU, true if there's a cluster on a project</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:group name="FinancialGroup">
		<xs:sequence>
			<xs:group ref="core:FinancialCoreGroup"/>
			<xs:element name="ProjectCostBreakdown" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ActivityCostBreakdown" type="costbreakdown:ActivityCostBreakDownType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="PreFinancingAmount" type="base:AmountType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>FP7, H2020 single prefinancing amount</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GuaranteeFundAmount" type="base:AmountType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>FP7, H2020 GF in case of single prefinancing amount</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ReimbursementRate" type="base:PercentageType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Required for H2020: This the reimbursement rate that is defined mainly by type of action or in some cases by topic</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Prefinancing" type="PrefinancingType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Required for H2020: some type of actions (eg.CoFund) require to have multiple prefinancings</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DetailedCostItemDefinition" type="costbreakdown:DetailedCostItemDefinitionType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="FinancialGuaranteeAmount" type="base:AmountType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Required for other programmes such as COSME, Health (3HP), Consumer (CP or CONS), RFCS. It may be requested to provide a financial guarantee from a bank or other third party. If there is an amount > 0, it means that financial guarantee has been requested for the grant.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FinancialGuaranteeDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>For projects having a FinancialGuaranteeAmount >0, this field holds the date when  the last required Financial Guarantee document was provided.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Invoices" type="invoice:InvoiceListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The list of invoices registered for a project</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IndirectCostsRate" type="base:PercentageType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Required for DG JUST financial information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FinancialGuarantee" type="FinancialGuaranteeType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Required for non H2020: some programs (eg.JUST) require to have multiple financial guarantees</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:group name="FundingSchemeGroup">
		<xs:sequence>
			<xs:group ref="core:FundingSchemeCoreGroup"/>
			<xs:element name="FundingSchemeSpecificInfo" type="FundingSchemeSpecificProjectInfoType" minOccurs="0"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="WorkPlanningGroup">
		<xs:sequence>
			<xs:element name="DescriptionOfWork" type="document:DocumentCoreType" minOccurs="0"/>
			<xs:element name="Deliverable" type="deliverable:DeliverableDefinitionType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Milestone" type="milestone:MilestoneDefinitionType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="WorkPackage" type="workpackage:WorkPlanItemType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="ReviewItem" type="review:ReviewDefinitionType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="RiskMitigation" type="riskmitigation:RiskMitigationDefinitionType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="SpecialClausesGroup">
		<xs:sequence>
