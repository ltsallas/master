                    <xs:documentation>Contains information for the Educational Program.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ErasmusECHEAccreditationFactList" type="ExtendedErasmusECHEAccreditationFactListType" minOccurs="0">
