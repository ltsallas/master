    <xs:complexType name="RatiosFactType">
        <xs:sequence>
            <xs:element name="Year" type="base:YearType">
                <xs:annotation>
                    <xs:documentation>The year of the figures used for computing these ratios.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Quick" type="BalanceSheetMetricType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>(debtors due in one year + cash at bank and in hand + other current assets) / (short term
                        non-bank debt + short term bank debt)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="GOProfit" type="BalanceSheetMetricType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Interest Paid / Gross Operating Profit/Loss</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ProfitabilityGross" type="BalanceSheetMetricType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Gross Operating Profit/Loss / TurnOver or Operating Income (if TurnOver = 0) also called
                        Profitability (1)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ProfitabilityNet" type="BalanceSheetMetricType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Net Operating Profit/Loss / TurnOver or Operating Income (if TurnOver = 0) also called
                        Profitability (2)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Solvency" type="BalanceSheetMetricType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Total Debt / (Capital and Reserves - (Intangible assets / 2))</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="EquityFlag" type="BalanceSheetMetricType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Equal to Solvency, but with different qualification</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ConciseAnalysis" type="AnalyticalMetricType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Overall score for financial capacity, based on concise financial viability and co-financing
                        checks using previous ratios
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DepthAnalysis" type="AnalyticalMetricType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Overall score for financial capacity, based on in-depth financial viability and co-financing
                        checks using previous ratios
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
