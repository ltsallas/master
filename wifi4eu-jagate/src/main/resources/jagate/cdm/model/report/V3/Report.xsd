		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IPRApplicationListType">
		<xs:sequence>
			<xs:element name="IPRApplication" type="IPRApplicationType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IPRApplicationType">
		<xs:sequence>
			<xs:element name="IPRType" type="xs:string"/>
			<xs:element name="AppRefPrefix" type="xs:string" minOccurs="0"/>
			<xs:element name="AppRefIdentifier" type="xs:string"/>
			<xs:element name="AppDate" type="xs:date"/>
			<xs:element name="AppTitle" type="xs:string"/>
			<xs:element name="ApplicantList" type="ApplicantListType"/>
			<xs:element name="IsAwarded" type="xs:string"/>
			<xs:element name="AwardPrefix" type="xs:string" minOccurs="0"/>
			<xs:element name="AwardIdentifier" type="xs:string" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="isNew" type="xs:boolean" default="false"/>
