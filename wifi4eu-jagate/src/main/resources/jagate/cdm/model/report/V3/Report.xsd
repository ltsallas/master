<xs:schema xmlns="http://ec.europa.eu/research/fp/model/report/V3" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:coderef="http://ec.europa.eu/research/fp/model/code-ref/V3" xmlns:riskmitigation="http://ec.europa.eu/research/fp/model/project-risk-mitigation/V1" xmlns:deliverableref="http://ec.europa.eu/research/fp/model/project-deliverable-ref/V1" xmlns:milestoneref="http://ec.europa.eu/research/fp/model/project-milestone-ref/V1" xmlns:costbreakdown="http://ec.europa.eu/research/fp/model/project-costbreakdown/V3" xmlns:base="http://ec.europa.eu/research/fp/model/base/V1" xmlns:document="http://ec.europa.eu/research/fp/model/document/V5" xmlns:expertref="http://ec.europa.eu/research/fp/model/expert-ref/V1" xmlns:legalentityref="http://ec.europa.eu/research/fp/model/legalentity-ref/V3" xmlns:reportingperiod="http://ec.europa.eu/research/fp/model/project-reportingperiod/V4" xmlns:projectmariecurie="http://ec.europa.eu/research/fp/model/project-mariecurie/V5" targetNamespace="http://ec.europa.eu/research/fp/model/report/V3" elementFormDefault="qualified" version="3.3.9">
	<xs:annotation>
		<xs:documentation>This schema intends to capture all data related to project related reports</xs:documentation>
	</xs:annotation>
	
	<xs:import namespace="http://ec.europa.eu/research/fp/model/base/V1" schemaLocation="../../base/V1/Base.xsd"/>
	<xs:import namespace="http://ec.europa.eu/research/fp/model/document/V5" schemaLocation="../../document/V5/Document.xsd"/>
	<xs:import namespace="http://ec.europa.eu/research/fp/model/expert-ref/V1" schemaLocation="../../expert-ref/V1/ExpertRef.xsd"/>
	<xs:import namespace="http://ec.europa.eu/research/fp/model/legalentity-ref/V3" schemaLocation="../../legalentity-ref/V3/LegalEntityRef.xsd"/>
	<xs:import namespace="http://ec.europa.eu/research/fp/model/code-ref/V3" schemaLocation="../../code-ref/V3/CodeRef.xsd"/>
	<xs:import namespace="http://ec.europa.eu/research/fp/model/project-milestone-ref/V1" schemaLocation="../../project-milestone-ref/V1/ProjectMilestoneRef.xsd"/>
	<xs:import namespace="http://ec.europa.eu/research/fp/model/project-deliverable-ref/V1" schemaLocation="../../project-deliverable-ref/V1/ProjectDeliverableRef.xsd"/>
	<xs:import namespace="http://ec.europa.eu/research/fp/model/project-reportingperiod/V4" schemaLocation="../../project-reportingperiod/V4/ProjectReportingPeriod.xsd"/>
	<xs:import namespace="http://ec.europa.eu/research/fp/model/project-costbreakdown/V3" schemaLocation="../../project-costbreakdown/V3/ProjectCostBreakdown.xsd"/>
	<xs:import namespace="http://ec.europa.eu/research/fp/model/project-risk-mitigation/V1" schemaLocation="../../project-risk-mitigation/V1/ProjectRiskMitigation.xsd"/>
	<xs:import namespace="http://ec.europa.eu/research/fp/model/project-mariecurie/V5" schemaLocation="../../project-mariecurie/V5/ProjectMarieCurie.xsd"/>
	
	<xs:simpleType name="ReportIdType">
		<xs:restriction base="xs:positiveInteger"/>
	</xs:simpleType>
	<xs:simpleType name="ReportStatusType">
		<xs:annotation>
			<xs:documentation>In SESAM there are slightly other statuses used: Uncompleted, Completed, (NEW to come for PICo: Validated (by system)), Submitted, Rejected.
		</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Accepted"/>
			<xs:enumeration value="Draft"/>
			<xs:enumeration value="Rejected"/>
			<xs:enumeration value="Submitted"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="ReportType" type="ReportType">
		<xs:annotation>
			<xs:documentation> type to be used for the final distribution report</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="ReportType">
		<xs:sequence>
			<xs:group ref="ReportGroup"/>
		</xs:sequence>
	</xs:complexType>
	<xs:group name="ReportGroup">
		<xs:sequence>
			<xs:element name="Created" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="CreatorUser" type="base:ECUserIdType" minOccurs="0"/>
			<xs:element name="DueInMonth" type="xs:positiveInteger" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Due In Month is the contractual month in project due for the report to be submitted. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DueDate" type="xs:date">
				<xs:annotation>
					<xs:documentation>The due date is either put or calculated from DueInMonth and startDate of the project.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DeliveryDate" type="xs:date">
				<xs:annotation>
					<xs:documentation>The delivery date is the date the report is delivered.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:complexType name="PeriodicReportType">
		<xs:annotation>
			<xs:documentation>type to be used for periodic report</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="ReportGroup"/>
			<xs:element name="CreatorPartner" type="legalentityref:LegalEntityIdType" minOccurs="0"/>
			<xs:element name="CreatedForPartner" type="legalentityref:LegalEntityIdType" minOccurs="0"/>
			<xs:element name="ReportingPeriod" type="reportingperiod:ReportingPeriodNumberType" nillable="0"/>
			<xs:element name="Content">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="CoverPage" type="ReportCoverPageType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Mandatory for SyGMa-generated Reports</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="AchievementsDeclaration" type="AchievementsDeclarationType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Mandatory for SyGMa-generated FP7 Reports</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="PublishableSummary" type="PublishableSummaryType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>PublishableSummary element is made optional, because ERC Periodic Reports will not use it</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="CoreDocument" type="document:DocumentType" minOccurs="0"/>
						<xs:element name="Deliverable" type="deliverableref:DeliverableRefType" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element name="DeliverablesRemarks" type="xs:string" minOccurs="0"/>
						<xs:element name="Milestone" type="milestoneref:MilestoneRefType" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element name="FinancialSummaryAndUOR" type="FinancialSummaryAndUORType" minOccurs="0"/>
						<xs:element name="FinancialSummaryDocument" type="document:DocumentCoreType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Check if this could be moved inside the FinancialSummaryAndUORType</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="FinancialStatements" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="PIC" type="legalentityref:LegalEntityIdType"/>
									<xs:element name="UTROPIC" type="legalentityref:LegalEntityIdType" minOccurs="0"/>
									<xs:element name="TPPIC" type="legalentityref:LegalEntityIdType" minOccurs="0"/>
									<xs:element name="FormCDocument">
										<xs:complexType>
											<xs:complexContent>
												<xs:extension base="document:DocumentCoreType"/>
											</xs:complexContent>
										</xs:complexType>
									</xs:element>
									<xs:element name="AdjustmentFormC" minOccurs="0" maxOccurs="unbounded">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="AdjustedReportingPeriod" type="reportingperiod:ReportingPeriodNumberType"/>
												<xs:element name="Document" type="document:DocumentCoreType"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="FinancialStatementCertificate" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="PIC" type="legalentityref:LegalEntityIdType"/>
									<xs:element name="UTROPIC" type="legalentityref:LegalEntityIdType" minOccurs="0"/>
									<xs:element name="TPPIC" type="legalentityref:LegalEntityIdType" minOccurs="0"/>
									<xs:element name="Document" type="document:DocumentCoreType"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="DeclaredAmountForJRC" type="base:AmountType" minOccurs="0"/>
						<xs:element name="Status" type="ReportStatusType" minOccurs="0"/>
						<xs:element name="StatusUpdated" type="xs:dateTime" minOccurs="0"/>
						<xs:element name="Document" type="document:DocumentType" minOccurs="0"/>
						<xs:element name="Questionnaires" type="QuestionnairesType" minOccurs="0"/>
						<xs:element name="CriticalRisks" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Header" type="SectionHeaderType"/>
									<xs:element name="UnforeseenRiskMitigation" minOccurs="0" maxOccurs="unbounded">
										<xs:complexType>
											<xs:complexContent>
												<xs:extension base="riskmitigation:RiskMitigationDefinitionType">
													<xs:attribute name="isNew" type="xs:boolean" default="false"/>
												</xs:extension>
											</xs:complexContent>
										</xs:complexType>
									</xs:element>
									<xs:element name="StateOfPlay" type="StateOfPlayType" minOccurs="0" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="FundsAvailabilityList" type="FundsAvailabilityListType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>List of Availability of Funds in case of multiprefinacing </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="MultiplePrefinancing" minOccurs="0">
							<xs:annotation>
								<xs:documentation> Declared consumption of the previous prefinancing</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="ConsumptionPrevAmount" type="base:AmountType" minOccurs="0">
										<xs:annotation>
											<xs:documentation> Declared consumption of the previous prefinancing as amount</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="ConsumptionPrevPerc" type="base:PercentageType">
										<xs:annotation>
											<xs:documentation> Declared consumption of the previous prefinancing as percentage</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="FinalFinancialSummary" type="FinancialSummaryAndUORType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Consolidated Financial Summary for the whole project, to be submitted in the last Periodic Report</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="PeriodicReportKind" type="coderef:CodeRefType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>It will be used to differentiate between ERC Scientific and 'Standard' periodic reports</xs:documentation>
				</xs:annotation>
			</xs:element>	
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FinalReportType">
		<xs:annotation>
			<xs:documentation>type to be used for the final report.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="ReportGroup"/>
			<xs:element name="CreatorPartner" type="legalentityref:LegalEntityIdType" minOccurs="0"/>
			<xs:element name="CreatedForPartner" type="legalentityref:LegalEntityIdType" minOccurs="0"/>
			<xs:element name="Content">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="PublishableSummary" type="PublishableSummaryType"/>
						<xs:element name="UseAndDissemination">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Document" type="document:DocumentCoreType" minOccurs="0"/>
									<xs:element name="Publication" type="PublicationType" minOccurs="0" maxOccurs="unbounded"/>
									<xs:element name="Patent" type="PatentType" minOccurs="0" maxOccurs="unbounded"/>
									<xs:element name="ExploitableForeground" type="ExploitableForegroundType" minOccurs="0" maxOccurs="unbounded"/>
									<xs:element name="DisseminationActivity" type="DisseminationActivityType" minOccurs="0" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="SocietalImplication" type="document:DocumentType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>these are the questionnaires</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Status" type="ReportStatusType" minOccurs="0"/>
						<xs:element name="StatusUpdated" type="xs:dateTime" minOccurs="0"/>
						<xs:element name="Document" type="document:DocumentType" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FinalDistributionReportType">
		<xs:annotation>
			<xs:documentation>type to be used for the final funding distribution report.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="ReportGroup"/>
			<xs:element name="CreatorPartner" type="legalentityref:LegalEntityIdType" minOccurs="0"/>
			<xs:element name="CreatedForPartner" type="legalentityref:LegalEntityIdType" minOccurs="0"/>
			<xs:element name="Content">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Status" type="ReportStatusType" minOccurs="0"/>
						<xs:element name="StatusUpdated" type="xs:dateTime" minOccurs="0"/>
						<xs:element name="Document" type="document:DocumentType" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:group name="ScientificActivityGroup">
		<xs:sequence>
			<xs:element name="Id" type="xs:positiveInteger"/>
			<xs:element name="ReportingPeriod" type="reportingperiod:ReportingPeriodNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Reporting Period Number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Created" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Timestamp of creation</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CreatorUser" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The User which created it</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Status" type="xs:string">
				<xs:annotation>
					<xs:documentation>PENDING, VALIDATED</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StatusUpdated" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Timestamp of when the status was updated</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Partner" type="legalentityref:LegalEntityIdType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>PIC</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Version" type="xs:positiveInteger"/>
			<xs:element name="Document" type="document:DocumentType" minOccurs="0"/>
		</xs:sequence>
	</xs:group>
	<xs:element name="Patent" type="PatentType"/>
	<xs:complexType name="PatentType">
		<xs:sequence>
			<xs:group ref="ScientificActivityGroup"/>
			<xs:element name="Applicants" type="xs:string"/>
			<xs:element name="Reference" type="xs:string"/>
			<xs:element name="Subject" type="xs:string">
				<xs:annotation>
					<xs:documentation>Type of IP rights: Patents, Trademarks, Registered designs, Utility models, etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Type" type="xs:string">
				<xs:annotation>
					<xs:documentation>Type of IP rights: Patents, Trademarks, Registered designs, Utility models, etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ConfidentialFlag" type="xs:boolean"/>
			<xs:element name="ForeseenEmbargoDate" type="xs:date" minOccurs="0"/>
			<xs:element name="URL" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ExploitableForeground" type="ExploitableForegroundType"/>
	<xs:complexType name="ExploitableForegroundType">
		<xs:sequence>
			<xs:group ref="ScientificActivityGroup"/>
			<xs:element name="Type" type="xs:string"/>
			<xs:element name="Description" type="xs:string"/>
			<xs:element name="ConfidentialFlag" type="xs:boolean"/>
			<xs:element name="Owner" type="xs:string">
				<xs:annotation>
					<xs:documentation>Owner and other beneficiaries involved</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ForeseenEmbargoDate" type="xs:date" minOccurs="0"/>
			<xs:element name="Patents" type="xs:string"/>
			<xs:element name="Product" type="xs:string"/>
			<xs:element name="Sector" type="xs:string"/>
			<xs:element name="Timetable" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Publication" type="PublicationType"/>
	<xs:complexType name="PublicationType">
		<xs:sequence>
			<xs:group ref="ScientificActivityGroup"/>
			<xs:element name="Order" type="xs:positiveInteger">
				<xs:annotation>
					<xs:documentation>Sequential numbering for display</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Author" type="xs:string"/>
			<xs:element name="PeriodicNumber" type="xs:string"/>
			<xs:element name="PeriodicTitle" type="xs:string"/>
			<xs:element name="PermanentIds" type="xs:string" minOccurs="0"/>
			<xs:element name="Place" type="xs:string" minOccurs="0"/>
			<xs:element name="Publisher" type="xs:string" minOccurs="0"/>
			<xs:element name="RelevantPages" type="xs:string"/>
			<xs:element name="Title" type="xs:string"/>
			<xs:element name="DateOfPublication" type="xs:date"/>
			<xs:element name="AccessOpen" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Indicates confidentiality</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="DisseminationActivity" type="DisseminationActivityType"/>
	<xs:complexType name="DisseminationActivityType">
		<xs:annotation>
			<xs:documentation>used in CP and BSG funding schemes</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="ScientificActivityGroup"/>
			<xs:element name="Order" type="xs:positiveInteger">
				<xs:annotation>
					<xs:documentation>Sequential numbering of the dissemination activity for display</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Title" type="xs:string">
				<xs:annotation>
					<xs:documentation>Title of the activity</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Date" type="xs:date">
				<xs:annotation>
					<xs:documentation>Date of the activity</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Place" type="xs:string">
				<xs:annotation>
					<xs:documentation>Place of the activity</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ActivityType" type="xs:string">
				<xs:annotation>
					<xs:documentation>Type of activity</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AudienceTypeList" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Type of audience</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="AudienceType" type="AudienceType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="AudienceSize" type="xs:nonNegativeInteger" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Size of audience</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CountriesAddressed" type="xs:string">
				<xs:annotation>
					<xs:documentation>Countries addressed by the dissemination activity</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AudienceType">
		<xs:sequence>
			<xs:element name="Name" type="xs:string"/>
			<xs:element name="Value" type="xs:boolean"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PublishableSummarySectionType">
		<xs:sequence>
			<xs:element name="Order" type="xs:int"/>
			<xs:element name="Title" type="xs:string"/>
			<xs:element name="Type" type="xs:string">
				<xs:annotation>
					<xs:documentation>ProjectContext, WorkPerformed, ExpectedResults, PublicWebsiteURL</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Body" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PublishableSummaryBodyType">
		<xs:sequence>
			<xs:element name="PublishableSummarySection" type="PublishableSummarySectionType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PublishableSummaryType">
		<xs:sequence>
			<xs:element name="Description" type="xs:string" minOccurs="0"/>
			<xs:element name="PublishableSummaryBody" type="PublishableSummaryBodyType"/>
			<xs:element name="Document" type="document:DocumentCoreType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ReviewType">
		<xs:annotation>
			<xs:documentation>type to be used for the project review.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Number" type="xs:positiveInteger">
				<xs:annotation>
					<xs:documentation>This is the review session Id which exists only when the session is created</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SessionTitle" type="xs:string"/>
			<xs:element name="SessionStartDate" type="xs:date"/>
			<xs:element name="SessionEndDate" type="xs:date" minOccurs="0"/>
			<xs:element name="SessionPlanned" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A flag to indicate if the review session was planned or not</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SessionStatus" type="xs:string">
				<xs:annotation>
					<xs:documentation>status can be OPEN, CLOSED, DELETED</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Report" type="ReviewReportType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="ReportingPeriod" type="reportingperiod:ReportingPeriodNumberType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ReviewReportType">
		<xs:annotation>
			<xs:documentation>type to be used for the review report done by expert</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Id" type="ReportIdType"/>
			<xs:element name="Type" type="xs:string">
				<xs:annotation>
					<xs:documentation>Individual, Consolidated</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Status" type="ReportStatusType" minOccurs="0"/>
			<xs:element name="StatusUpdated" type="xs:dateTime" minOccurs="0"/>
