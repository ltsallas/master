		<xs:restriction base="xs:string">
			<xs:enumeration value="FULLY_ACHIEVED"/>
			<xs:enumeration value="MOSTLY_ACHIEVED"/>
			<xs:enumeration value="FAILED_TO_ACHIEVE"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="AchievementsDeclarationType">
		<xs:annotation>
			<xs:documentation>type to be used for the FP7 Coordinator Declaration about Project Achievements</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ObjectivesAchievement" type="ObjectivesAchievementType"/>
			<xs:element name="PublicWebsiteUpToDate" type="xs:boolean" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FinancialSummaryAndUORType">
		<xs:annotation>
			<xs:documentation>type to be used for the Periodic Report Financial Summary</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FinancialSummaryAndUORPerPartner" type="FinancialSummaryAndUORPerPartnerType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Aggregated information from all the Form Cs related to a partner</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FinancialSummaryProjectTotals" type="costbreakdown:ActivityCostBreakDownType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Aggregated costs from all the submitted Form Cs ('Total' row of the Financial Summary)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FinancialSummaryAndUORPerPartnerType">
		<xs:sequence>
			<xs:element name="PIC" type="legalentityref:LegalEntityIdType"/>
			<xs:element name="UTROPIC" type="legalentityref:LegalEntityIdType" minOccurs="0"/>
			<xs:element name="TPPIC" type="legalentityref:LegalEntityIdType" minOccurs="0"/>
			<xs:element name="FinancialSummaryPerPartnerFormC" type="FinancialSummaryPerPartnerFormCType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Aggregated information about a single Form C submitted by a partner (1 row of the Financial Summary)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="UseOfResourcesPerPartner" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="UseOfResource" type="costbreakdown:UseOfResourceWithCostCategory" maxOccurs="unbounded"/>
						<xs:element name="UseOfResourcesTotal" type="base:AmountType"/>
						<xs:element name="IsAdjustment" type="xs:boolean" minOccurs="0"/>
						<xs:element name="AdjustmentPeriod" type="reportingperiod:ReportingPeriodNumberType" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="UseOfResourcesPerPartnerH2020FS" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Each of these elements corresponds to the UOR for a Financial Statement submitted by the legal entity</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="UseOfResourcePerPartnerCategoryH2020" type="costbreakdown:UseOfResourceH2020Type" maxOccurs="unbounded"/>
						<xs:element name="isAdjustment" type="xs:boolean" minOccurs="0"/>
						<xs:element name="AdjustmentPeriod" type="reportingperiod:ReportingPeriodNumberType" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FinancialSummaryPerPartnerFormCType">
		<xs:annotation>
			<xs:documentation>type that contains the information of a Form C that will be used in a Financial Summary</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="IsAdjustment" type="xs:boolean"/>
			<xs:element name="ActivityCost" type="costbreakdown:ActivityCostBreakDownType" maxOccurs="unbounded"/>
			<xs:element name="InterestAmount" type="base:AmountType" minOccurs="0"/>
			<xs:element name="AdjustmentPeriod" type="reportingperiod:ReportingPeriodNumberType" minOccurs="0"/>
			<xs:element name="AdjustmentType" type="coderef:CodeRefType" minOccurs="0"/>
			<xs:element name="ReimbursementRate" type="base:PercentageType" minOccurs="0"/>
			<xs:element name="HasLRICertificate" type="xs:boolean" minOccurs="0"/>
			<xs:element name="IsCFSRequired" type="xs:boolean" minOccurs="0"/>
			<xs:element name="HasCFS" type="xs:boolean" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="QuestionnairesType">
		<xs:sequence>
			<xs:element name="SMEPhase1" type="SMEPhase1QuestionnaireType" minOccurs="0"/>
			<xs:element name="DisseminationAndCommunication" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Header" type="SectionHeaderType"/>
						<xs:element name="TotalFunding" type="QuestionType"/>
						<xs:element name="NumberOfActivities" type="StructuredQuestionType"/>
						<xs:element name="EstimatedAudienceReach" type="StructuredQuestionType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Innovation" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Header" type="SectionHeaderType"/>
						<xs:element name="DevelopedActivities" type="StructuredQuestionType" minOccurs="0"/>
						<xs:element name="InnovationToTheMarket" type="StructuredQuestionType" minOccurs="0"/>
						<xs:element name="PrivateCompanies" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="InnovatingMarket" type="QuestionType"/>
									<xs:element name="InnovatingMarketSMEs" type="QuestionType"/>
									<xs:element name="InnovatingCompany" type="QuestionType"/>
									<xs:element name="InnovatingCompanySMEs" type="QuestionType"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Gender" type="GenderQuestionnaireType" minOccurs="0"/>
			<xs:element name="OpenResearchData" type="OpenResearchDataQuestionnaireType" minOccurs="0"/>
			<xs:element name="IPR" type="IPRQuestionnaireType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SMEPhase1QuestionnaireType">
		<xs:sequence>
			<xs:element name="Header" type="SectionHeaderType"/>
			<xs:element name="CoachingActivities">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="DidBenefit" type="QuestionType"/>
						<xs:element name="Rating" type="QuestionType" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="IdeaPursuit">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="WillPursue" type="QuestionType"/>
						<xs:element name="ReasonsToNotPursue" type="StructuredQuestionType" minOccurs="0"/>
						<xs:element name="SourceOfFinance" type="StructuredQuestionType" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="EstimatedForecast">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Label" type="xs:string"/>
						<xs:element name="SMEGrowth" type="SMESurveyListType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="GrantResults" type="StructuredQuestionType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SectionHeaderType">
		<xs:sequence>
			<xs:element name="SectionNumber" type="xs:string" minOccurs="0"/>
			<xs:element name="SectionTitle" type="xs:string"/>
			<xs:element name="NoDataLabel" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="QuestionType">
		<xs:sequence>
			<xs:element name="OrderNumber" type="xs:int" minOccurs="0"/>
			<xs:element name="Label" type="xs:string"/>
			<xs:choice>
				<xs:element name="Value" type="xs:string"/>
				<xs:element name="BoolValue" type="xs:boolean"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="StructuredQuestionType">
		<xs:sequence>
			<xs:element name="OrderNumber" type="xs:int" minOccurs="0"/>
			<xs:element name="Label" type="xs:string" minOccurs="0"/>
			<xs:element name="SubQuestion" type="QuestionType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="StateOfPlayType">
		<xs:sequence>
			<xs:element name="RiskMitigationRef" type="riskmitigation:RiskMitigationRefType"/>
			<xs:element name="PeriodNumber" type="reportingperiod:ReportingPeriodNumberType"/>
			<xs:element name="AppliedMeasures" type="xs:boolean"/>
			<xs:element name="RiskMaterialise" type="xs:boolean"/>
			<xs:element name="Comments" type="xs:string" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="isNew" type="xs:boolean" default="false"/>
	</xs:complexType>
	<xs:complexType name="GenderQuestionnaireType">
		<xs:sequence>
			<xs:element name="Header" type="SectionHeaderType"/>
			<xs:element name="GenderDimension" type="QuestionType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OpenDataSetType">
		<xs:sequence>
			<xs:element name="DOI" type="xs:string" minOccurs="0"/>
			<xs:element name="Identifier" type="xs:string" minOccurs="0"/>
			<xs:element name="Accessible" type="xs:boolean"/>
			<xs:element name="Reusable" type="xs:boolean"/>
			<xs:element name="RelatedPublicationDOI" type="xs:string" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="isNew" type="xs:boolean" default="false"/>
	</xs:complexType>
	<xs:complexType name="OpenResearchDataQuestionnaireType">
		<xs:sequence>
			<xs:element name="Header" type="SectionHeaderType"/>
			<xs:element name="OpenDataSet" type="OpenDataSetType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SMESurveyType">
		<xs:sequence>
			<xs:element name="SurveyType" type="xs:string">
				<xs:annotation>
					<xs:documentation>Possible values: BEGINNING_OF_PROJECT, ACCOUNTABILITY_PERIOD. YEARLY_FORECAST </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SurveyPeriod" type="xs:integer" minOccurs="0"/>
			<xs:element name="Turnover" type="xs:string"/>
			<xs:element name="Employment" type="xs:string"/>
		</xs:sequence>
		<xs:attribute name="newToReport" type="xs:boolean" default="false"/>
	</xs:complexType>
	<xs:complexType name="SMESurveyListType">
		<xs:sequence>
			<xs:element name="SMESurvey" type="SMESurveyType" maxOccurs="unbounded"/>
		</xs:sequence>
