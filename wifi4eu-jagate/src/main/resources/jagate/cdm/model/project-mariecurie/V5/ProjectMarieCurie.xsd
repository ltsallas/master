					<xs:element name="ResearchersOverviewPerPeriod" type="ResearchersOverviewPerPeriodType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="Group" type="coderef:CodeRefType">
						<xs:annotation>
							<xs:documentation>use of CCM Class Id: MCResearchersOverviewGroup</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
