	<xs:element name="MarieCurieProjectInfo" type="MarieCurieProjectInfoType"/>
	<xs:complexType name="MarieCurieProjectInfoType">
		<xs:annotation>
			<xs:documentation>Project level data elements specific to MarieCurie projects.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ResearchClassifiedAsLaboratoryBased" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>FP7</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ScientificPanelCode" type="coderef:CodeRefType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>code of type "ScientificArea"</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FinancialContributionPerYear" type="MarieCurieFinancialContributionPerYearType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>FP7</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FinancialContribution" type="MarieCurieFinancialContributionType"/>
			<xs:element name="ActionCode" type="coderef:CodeRefType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>FP7: code of type "MCActionCode"</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>FP7: CoFundFellowship if SubFundingScheme is COFUND</xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element name="Fellowship" type="MarieCurieFellowshipType"/>
					<xs:element name="CoFundFellowship" type="MarieCurieCoFundFellowshipType"/>
				</xs:sequence>
			</xs:choice>
			<xs:element name="IndividualResearcher" type="ResearcherType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>for Individual Fellowships</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ResearcherOverview" type="MarieCurieResearcherOverviewType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>FP7: This is used in SubFundingSchemes IAPP and ITN to have an overview of all researchers in the project.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AverageTravelAllowance" type="base:AmountType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>FP7: This is used in SubFundingScheme IAPP in case the travel distance is unknown.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Recruitment" type="RecruitmentType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Secondment" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MarieCurieProjectInfoH2020Type">
