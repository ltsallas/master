					<xs:documentation>code of type "ScientificArea"</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IndividualResearcher" type="IndividualResearcherType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>for Individual Fellowships</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Recruitment" type="RecruitmentType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Secondment" type="SecondmentType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="PartnerOrganisation" type="partner:PartnerOrganisationType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="ProjectResearchersOverview" type="ProjectResearchersOverviewType" minOccurs="0"/>
