			<xs:element name="PartTimeSuspensions" type="PartTimeSuspensionsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Part-time suspensions of MSCA-IF ToAs</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
