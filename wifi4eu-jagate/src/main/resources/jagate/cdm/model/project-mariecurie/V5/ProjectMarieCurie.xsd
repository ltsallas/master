					<xs:element name="ResearchersOverviewPerPeriod" type="ResearchersOverviewPerPeriodType" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ProjectResearchersOverviewPerGroupType">
		<xs:complexContent>
