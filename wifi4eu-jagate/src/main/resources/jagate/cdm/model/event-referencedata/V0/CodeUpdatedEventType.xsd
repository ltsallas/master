<xs:schema xmlns:eventbase="http://ec.europa.eu/rdg/efp/domain/event/v1_0" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:base="http://ec.europa.eu/research/fp/model/base/V1" xmlns:code="http://ec.europa.eu/research/fp/model/code/V3" xmlns:ns1="http://ec.europa.eu/rdg/efp/domain/event/referencedata/codeupdated-event/V1" xmlns:ns2="http://ec.europa.eu/rdg/efp/domain/event/referencedata/codeupdated-event/V0" targetNamespace="http://ec.europa.eu/rdg/efp/domain/event/referencedata/codeupdated-event/V0" elementFormDefault="qualified" version="0.0.0">
	<xs:import namespace="http://ec.europa.eu/rdg/efp/domain/event/v1_0" schemaLocation="../../eventbase/V0/EventBase.xsd"/>
	<xs:import namespace="http://ec.europa.eu/research/fp/model/base/V1" schemaLocation="../../base/V1/Base.xsd"/>
	<xs:import namespace="http://ec.europa.eu/research/fp/model/code/V3" schemaLocation="../../code/V3/Code.xsd"/>
	<xs:complexType name="CodeUpdatedEventType">
		<xs:annotation>
			<xs:documentation>Event generated when a code is updated</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="eventbase:EventBodyType">
				<xs:sequence>
					<xs:element name="oldValue" type="code:CodeType">
						<xs:annotation>
							<xs:documentation>Complete description of the old version of the code (before the update)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="newValue" type="code:CodeType">
						<xs:annotation>
							<xs:documentation>Complete description of the new version of the code after the update)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="UpdateDate" type="xs:dateTime">
						<xs:annotation>
							<xs:documentation>Moment of the official update: an updated code is considered valid from this official update date (this date should typically be  very close or identical to the event emission date)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Comments" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Optional text explaining the context or reason for the update of this code</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>v 1.0.0: Creation </xs:documentation>
	</xs:annotation>
</xs:schema>
