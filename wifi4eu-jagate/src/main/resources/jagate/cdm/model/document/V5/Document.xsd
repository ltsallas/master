			<xs:element name="Date" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="HashCode" type="xs:long" minOccurs="0"/>
			<xs:element name="VersionNo" type="xs:int" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
