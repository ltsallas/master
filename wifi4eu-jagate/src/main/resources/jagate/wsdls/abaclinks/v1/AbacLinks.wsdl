		<wsdl:operation name="GetAbacPrintOutLink">
			<wsdl:input>
				<soap:body use="literal" />
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" />
			</wsdl:output>
			<wsdl:fault name="AbacLinkException">
				<soap:fault name="AbacLinkException" use="literal" />
			</wsdl:fault>
		</wsdl:operation>		
	</wsdl:binding>	

	<wsdl:service name="AbacLinksService">
		<wsdl:port binding="IAbacLinksServiceBinding" name="AbacLinksServiceBinding">
