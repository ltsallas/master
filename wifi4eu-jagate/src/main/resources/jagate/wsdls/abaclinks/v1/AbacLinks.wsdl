	</wsdl:message>

	<wsdl:message name="GetPrintOutLinkResponse">
		<wsdl:part element="abaclink:GetPrintOutLinkResponse" name="response" />
	</wsdl:message>	
		
	
	<wsdl:portType name="IAbacLinksService">
		<wsdl:operation name="GetAbacDeepLink">
			<wsdl:input message="GetDeepLinkRequest" />
			<wsdl:output message="GetDeepLinkResponse" />
			<wsdl:fault name="AbacLinkException" message="AbacLinkException"/>
