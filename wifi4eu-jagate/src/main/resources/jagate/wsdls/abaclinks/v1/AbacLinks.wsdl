			<xs:import namespace="http://ec.europa.eu/rdg/jagate/ws/domain/base/v2" schemaLocation="../../../domain/base/v2/JAgateBase.xsd" />
		</xs:schema>
	</wsdl:types>

	<wsdl:message name="AbacLinkException">
		<wsdl:part element="jagate:Exception" name="Exception" />
	</wsdl:message>	

	<wsdl:message name="GetDeepLinkRequest">
		<wsdl:part element="abaclink:GetAbacLinkRequest" name="request" />
