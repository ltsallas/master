			<xs:import namespace="http://ec.europa.eu/rdg/jagate/ws/domain/base/v2" schemaLocation="../../../domain/base/v2/JAgateBase.xsd" />
		
			<!-- <xs:import namespace="http://ec.europa.eu/rdg/jagate/ws/prefinancing/v1/interfaces" schemaLocation="PrefinancingInterface.xsd" /> -->
		</xs:schema>
	</wsdl:types>

	<!-- Messages -->
	<wsdl:message name="CreatePrefinancingGuaranteeFundJRCRequest">
		<wsdl:part element="prefinGf:CreatePrefinancingGuaranteeFundJRCRequest" name="request" />
	</wsdl:message>
	<wsdl:message name="CreatePrefinancingRequest">
		<wsdl:part element="prefin:CreatePrefinancingRequest" name="request" />
	</wsdl:message>
	<wsdl:message name="CreatePrefinancingJRCRequest">
