	</wsdl:message>
	<wsdl:message name="CreatePrefinancingResponse">
		<wsdl:part element="prefin:CreatePrefinancingResponse" name="response" />
	</wsdl:message>
	<wsdl:message name="CreatePrefinancingException">
		<wsdl:part element="jagate:Exception" name="exception" />
	</wsdl:message>	
	
	<wsdl:portType name="IPrefinancingService">
		<wsdl:documentation>Creates the Prefinancing with the given parameters.</wsdl:documentation>
		<wsdl:operation name="CreatePrefinancing">
			<wsdl:documentation>For Coordinator prefin</wsdl:documentation>
			<wsdl:input message="CreatePrefinancingRequest" />
			<wsdl:output message="CreatePrefinancingResponse" />
			<wsdl:fault name="CreatePrefinancingException" message="CreatePrefinancingException"/>
		</wsdl:operation>
		<wsdl:operation name="CreatePrefinancingJRC">
			<wsdl:documentation>For JRC</wsdl:documentation>
			<wsdl:input message="CreatePrefinancingJRCRequest" />
			<wsdl:output message="CreatePrefinancingResponse" />
			<wsdl:fault name="CreatePrefinancingException" message="CreatePrefinancingException"/>
		</wsdl:operation>
		<wsdl:operation name="CreatePrefinancingGuaranteeFundJRC">
			<wsdl:documentation>GuaranteeFundFor JRC</wsdl:documentation>
			<wsdl:input message="CreatePrefinancingGuaranteeFundJRCRequest" />
			<wsdl:output message="CreatePrefinancingGuaranteeFundJRCResponse" />
			<wsdl:fault name="CreatePrefinancingException" message="CreatePrefinancingException"/>
		</wsdl:operation>
	</wsdl:portType>

	<!-- Binding -->
	<wsdl:binding name="IPrefinancingServiceBinding" type="IPrefinancingService">
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
		<wsdl:operation name="CreatePrefinancing">
			<wsdl:input>
