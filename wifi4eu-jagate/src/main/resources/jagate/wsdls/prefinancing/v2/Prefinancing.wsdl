			<wsdl:output>
				<soap:body use="literal" />
			</wsdl:output>
			<wsdl:fault name="CreatePrefinancingException">
				<soap:fault name="CreatePrefinancingException" use="literal" />
			</wsdl:fault>
		</wsdl:operation>
	</wsdl:binding>

	<!-- Service definition -->
	<wsdl:service name="PrefinancingServiceV2">
		<wsdl:port binding="IPrefinancingServiceBinding" name="PrefinancingServiceBinding">
