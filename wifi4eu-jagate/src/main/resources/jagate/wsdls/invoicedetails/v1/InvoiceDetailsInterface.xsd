           xmlns:jagref="http://ec.europa.eu/rdg/jagate/ws/domain/reference/v1"
		   targetNamespace="http://ec.europa.eu/rdg/jagate/ws/invoicedetails/v1/interfaces"
		   elementFormDefault="qualified">
		   
		   <!-- xmlns:invoice="http://ec.europa.eu/rdg/jagate/ws/domain/invoiceBasicData/v1"-->
		   
	<xs:import namespace="http://ec.europa.eu/rdg/jagate/ws/domain/base/v2" schemaLocation="../../../domain/base/v2/JAgateBase.xsd"/>
	<xs:import namespace="http://ec.europa.eu/rdg/jagate/ws/domain/reference/v1" schemaLocation="../../../domain/reference/v1/ReferenceData.xsd"/>
	<xs:import namespace="http://ec.europa.eu/rdg/jagate/ws/domain/commitment/v2" schemaLocation="../../../domain/commitment/v2/Commitment.xsd"/>
