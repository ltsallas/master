	<xs:complexType name="GetInvoiceDetailsRequestType">
		<xs:sequence>
			<xs:element name="invoiceId" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="GetInvoiceDetailsResponseType">
		<xs:sequence>
			<xs:element name="invoice" type="invoice:InvoiceType"/>
		</xs:sequence>
	</xs:complexType>
