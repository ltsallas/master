<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
		   xmlns="http://ec.europa.eu/rdg/jagate/ws/domain/event/bankaccount/finish/v1"
		   targetNamespace="http://ec.europa.eu/rdg/jagate/ws/domain/event/bankaccount/finish/v1"
		   xmlns:eventbase="http://ec.europa.eu/rdg/jagate/ws/domain/event/base/v2"
		   xmlns:ba="http://ec.europa.eu/rdg/jagate/ws/domain/bankaccount/v4"
		   elementFormDefault="qualified" 
		   attributeFormDefault="unqualified">
		   
    <xs:import namespace="http://ec.europa.eu/rdg/jagate/ws/domain/event/base/v2" schemaLocation="../../base/v2/JagateBaseEvent.xsd" />
    <xs:import namespace="http://ec.europa.eu/rdg/jagate/ws/domain/bankaccount/v4" schemaLocation="../../../bankaccount/v4/BankAccount.xsd" />

	<xs:complexType name="BACreateOrUpdateFinishedEvent">
		<xs:complexContent>			
			<xs:extension base="eventbase:JagateAbstractEvent">
				<xs:sequence>
					<xs:element name="requestId" type="xs:string"  minOccurs="0">
						<xs:annotation>
							<xs:documentation>Temporary id, optional. Present if the update came from a request initiated by a Jagate client (e.g. PDM) only). Not present for an update event initiated elsewhere (e.g. directly in the Abac application).</xs:documentation>
						</xs:annotation>
					</xs:element>						
					<xs:element name="id" type="xs:string"/>
					<xs:element name="BlockedInfo" type="ba:BlockedInfoType" minOccurs="0"/>
					<xs:element name="EWS" type="ba:EarlyWarningType" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


</xs:schema>