           xmlns:eventbase="http://ec.europa.eu/rdg/jagate/ws/domain/event/base/v4"
           elementFormDefault="qualified"
           attributeFormDefault="unqualified"
           version="1.0">

    <xs:import namespace="http://ec.europa.eu/rdg/jagate/ws/domain/event/base/v4" schemaLocation="../../base/v4/JagateBaseEvent.xsd" />

    <xs:complexType name="BankAccountCreateLEBALinkEvent">
