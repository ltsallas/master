		   targetNamespace="http://ec.europa.eu/rdg/jagate/ws/domain/event/base/visastatus/v2"
		   xmlns:visa="http://ec.europa.eu/rdg/jagate/ws/domain/visa/v1"
		   elementFormDefault="qualified" 
		   attributeFormDefault="unqualified">

	<xs:import namespace="http://ec.europa.eu/rdg/jagate/ws/domain/visa/v1" schemaLocation="../../../visa/v1/Visa.xsd" />

	<xs:complexType name="VisaStatus">
		<xs:sequence>
			<xs:element name="visa" type="visa:AgentType"/>
