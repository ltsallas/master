<?xml version="1.0" encoding="UTF-8"?>
<xs:schema elementFormDefault="qualified" version="1.0.0" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
	xmlns="http://ec.europa.eu/rdg/jagate/ws/domain/reference/v1" targetNamespace="http://ec.europa.eu/rdg/jagate/ws/domain/reference/v1" jaxb:version="2.1">
	<xs:annotation>
		<xs:documentation>
			Defines Reference data types.
		</xs:documentation>
	</xs:annotation>

	<xs:simpleType name="EMessageType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="INFO" />
			<xs:enumeration value="WARNING" />
			<xs:enumeration value="ERROR" />
		</xs:restriction>
	</xs:simpleType>


	<xs:simpleType name="EContractStatusType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Cancelled" />
			<xs:enumeration value="Ended" />
			<xs:enumeration value="Draft" />
			<xs:enumeration value="Terminated" />
			<xs:enumeration value="Signed by Contractor(s)" />
			<xs:enumeration value="In Force" />
			<xs:enumeration value="Submitted" />
			<xs:enumeration value="Si2 Committed" />
			<xs:enumeration value="Amended" />
			<xs:enumeration value="Sent to Contractor" />
			<xs:enumeration value="Suspended" />
			<xs:enumeration value="Linked To Commitment" />
			<xs:enumeration value="Approved" />
			<xs:enumeration value="Not Applicable" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="EContractTechnicalStatusType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Successfully Verified" />
			<xs:enumeration value="Not Applicable" />
			<xs:enumeration value="New" />
			<xs:enumeration value="Pending" />
			<xs:enumeration value="Busy" />
			<xs:enumeration value="Completely processed" />
			<xs:enumeration value="Error" />
			<xs:enumeration value="Confirmed" />
			<xs:enumeration value="Partially processed" />
			<xs:enumeration value="Submitted" />
			<xs:enumeration value="Ready" />
			<xs:enumeration value="Draft" />
			<xs:enumeration value="Warning" />
			<xs:enumeration value="To be confirmed" />
			<xs:enumeration value="To be validated" />
			<xs:enumeration value="UnSuccessfully Verified" />
			<xs:enumeration value="Blocked" />
			<xs:enumeration value="Not Validated" />
			<xs:enumeration value="Not Confirmed" />
			<xs:enumeration value="Pre-Draft" />
		</xs:restriction>
	</xs:simpleType>

    <xs:simpleType name="FractionDigits">
        <xs:restriction base="xs:decimal">
            <xs:fractionDigits value="6" />
        </xs:restriction>
    </xs:simpleType>

	<xs:simpleType name="EWorkflowStepNameType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="agate_ack" />
			<xs:enumeration value="agate_invoice_ack" />
			<xs:enumeration value="waiting_for_abac" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="EMessagingType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Queue" />
			<xs:enumeration value="Topic" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="EClientTransactionType">
		<xs:annotation>
			<xs:documentation>This is client related transaction name like Payment, Invoice, Commitment amd so on..We don't specify whether it
				is Payment Request or
				Order, we stay high level.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Contract" />
			<xs:enumeration value="Commitment" />
			<xs:enumeration value="Invoice" />
			<xs:enumeration value="Payment" />
			<xs:enumeration value="Prefinancing document" />
			<xs:enumeration value="Audit" />
			<xs:enumeration value="Recovery order" />
			<xs:enumeration value="Visa" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="EAgentType">
		<xs:annotation>
			<xs:documentation>The agent type giving his approval/refusal. This is
				based on ABAC security roles. This enumeration lists the possible
				values
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="IA">
				<xs:annotation>
					<xs:documentation>Initiating agent</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DEA">
				<xs:annotation>
					<xs:documentation>Data Entry Agent</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OIA">
				<xs:annotation>
					<xs:documentation>Operational Initiating Agent</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FIA">
				<xs:annotation>
					<xs:documentation>Financial Initiating Agent</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OVA">
				<xs:annotation>
					<xs:documentation>Operational Verifying Agent</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FVA">
				<xs:annotation>
					<xs:documentation>Financial Verifying Agent</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

