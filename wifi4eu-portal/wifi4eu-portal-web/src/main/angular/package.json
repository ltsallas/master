{
  "name": "angular",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",

    "clean": "rm -rf dist",
    "build": "tsc -p src/tsconfig.json",

    "rollup_vendor": "rollup -f iife -c rollup/vendor.rollup.config.js",
    "minify_vendor": "minify dist/vendor.bundle.js --output dist/vendor.bundle.js",
    "uglify_vendor": "uglifyjs dist/vendor.bundle.js --screw-ie8 --compress --mangle --output dist/vendor.bundle.js",
    "compress_vendor": "npm run rollup_vendor && npm run minify_vendor && npm run uglify_vendor",

    "rollup_styles": "rollup -f iife -c rollup/styles.rollup.config.js",
    "uglify_styles": "uglifyjs dist/styles.bundle.js --screw-ie8 --compress --mangle --output dist/styles.bundle.js",
    "compress_styles": "npm run rollup_styles && npm run uglify_styles",

    "rollup_main": "rollup -f iife -c rollup/main.rollup.config.js",
    "uglify_main": "uglifyjs dist/main.bundle.js --screw-ie8 --compress --mangle --output dist/main.bundle.js",
    "compress_main": "npm run rollup_main && npm run uglify_main",

    "rollup_common": "rollup -f iife -c rollup/common.rollup.config.js",
    "uglify_common": "uglifyjs dist/common.chunk.js --screw-ie8 --compress --mangle --output dist/common.chunk.js",
    "compress_common": "npm run rollup_common && npm run uglify_common",

    "rollup_dgconnportal": "rollup -f iife -c rollup/dgconnportal.rollup.config.js",
    "uglify_dgconnportal": "uglifyjs dist/dgconnportal.module.chunk.js --screw-ie8 --compress --mangle --output dist/dgconnportal.module.chunk.js",
    "compress_dgconnportal": "npm run rollup_dgconnportal && npm run uglify_dgconnportal",

    "rollup_supplierportal": "rollup -f iife -c rollup/supplier-portal.rollup.config.js",
    "uglify_supplierportal": "uglifyjs dist/supplier-portal.module.chunk.js --screw-ie8 --compress --mangle --output dist/supplier-portal.module.chunk.js",
    "compress_supplierportal": "npm run rollup_supplierportal && npm run uglify_supplierportal",

    "rollup_beneficiaryportal": "rollup -f iife -c rollup/beneficiary-portal.rollup.config.js",
    "uglify_beneficiaryportal": "uglifyjs dist/beneficiary-portal.module.chunk.js --screw-ie8 --compress --mangle --output dist/beneficiary-portal.module.chunk.js",
    "compress_beneficiaryportal": "npm run rollup_beneficiaryportal && npm run uglify_beneficiaryportal",

    "build_vendor_testing": "npm run clean && ng build --env=local && npm run build && npm run compress_vendor",
    "build_styles_testing": "npm run clean && ng build --env=local && npm run build && npm run compress_styles",
    "build_main_testing": "npm run clean && ng build --env=local && npm run build && npm run compress_main",
    "build_common_testing": "npm run clean && ng build --env=local && npm run build && npm run compress_common",
    "build_dgconnportal_testing": "npm run clean && ng build --env=local && npm run build && npm run compress_dgconnportal",
    "build_supplierportal_testing": "npm run clean && ng build --env=local && npm run build && npm run compress_supplierportal",
    "build_beneficiaryportal_testing": "npm run clean && ng build --env=local && npm run build && npm run compress_beneficiaryportal"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.4.4",
    "@angular/common": "^4.2.6",
    "@angular/compiler": "^4.2.6",
    "@angular/core": "^4.2.6",
    "@angular/forms": "^4.2.6",
    "@angular/http": "^4.2.6",
    "@angular/platform-browser": "^4.2.6",
    "@angular/platform-browser-dynamic": "^4.2.6",
    "@angular/router": "^4.2.6",
    "@ec-digit-uxatec/eui-angular2-ux-animation": "^1.4.6",
    "@ec-digit-uxatec/eui-angular2-ux-commons": "^1.4.6",
    "@ec-digit-uxatec/eui-angular2-ux-http-interceptor": "^1.4.6",
    "@ec-digit-uxatec/eui-angular2-ux-language-selector": "^1.4.6",
    "@ec-digit-uxatec/eui-angular2-ux-search-input": "^1.4.6",
    "@ec-digit-uxatec/eui-styles": "^1.4.6",
    "@ngx-translate/core": "^9.0.2",
    "@types/crypto-js": "^3.1.33",
    "@types/file-saver": "0.0.1",
    "angular-2-local-storage": "^1.0.1",
    "chart.js": "^2.5.0",
    "class-transformer": "^0.1.2",
    "core-js": "^2.4.1",
    "crypto-js": "^3.1.9-1",
    "file-saver": "^1.3.3",
    "moment": "^2.16.0",
    "ng2-bs3-modal": "^0.10.4",
    "ng2-charts": "^1.5.0",
    "ng2-google-recaptcha": "^1.2.0",
    "ng2-translate": "^4.0.1",
    "primeng": "^1.1.1",
    "rxjs": "5.0.1",
    "ts-helpers": "^1.1.1",
    "typings": "^2.1.1",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.2.0",
    "@angular/compiler-cli": "^4.2.6",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "codelyzer": "~2.0.0-beta.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "~4.0.13",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "~2.3.2"
  }
}
